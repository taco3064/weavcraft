{
  "name": "core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/core/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/core",
        "tsConfig": "libs/core/tsconfig.lib.json",
        "project": "libs/core/package.json",
        "entryFile": "libs/core/src/index.ts",
        "external": [
          "react",
          "react-dom",
          "@emotion/react/jsx-runtime"
        ],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "libs/core/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "tsc": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsc --noEmit --declaration false --composite false -p libs/core/tsconfig.lib.json",
          "tsc --noEmit --declaration false --composite false -p libs/core/tsconfig.spec.json"
        ]
      }
    }
  }
}
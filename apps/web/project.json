{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "targets": {
    "postbuild": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "next-sitemap --config apps/web/next-sitemap.config.js"
        ]
      }
    },
    "tsc": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsc --noEmit -p apps/web/tsconfig.json"
        ]
      }
    }
  },
  "tags": []
}